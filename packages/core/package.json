{
    "name": "@tinyburg/core",
    "version": "0.0.1-alpha",
    "description": "api for the mobile game TinyTower by Nimblebit",
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "main": "dist/tinyburg.js",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "heft build --clean",
        "lint": "eslint .",
        "update-common-data": "ts-node --esm bin/update-common-data.ts"
    },
    "dependencies": {
        "debug": "~4.3.4",
        "deep-extend": "~0.6.0",
        "got": "~12.5.3",
        "prompts": "~2.4.2",
        "semver": "~7.3.8",
        "supports-color": "~9.3.0"
    },
    "devDependencies": {
        "@rushstack/eslint-config": "~3.1.1",
        "@rushstack/heft": "~0.48.8",
        "@tinyburg/insight": "workspace:*",
        "@tsconfig/node18-strictest-esm": "~1.0.1",
        "@types/debug": "~4.1.7",
        "@types/deep-extend": "~0.6.0",
        "@types/node": "~18.11.11",
        "@types/prompts": "~2.4.2",
        "@types/semver": "~7.3.13",
        "eslint": "~8.29.0",
        "ts-node": "~10.9.1",
        "typescript": "~4.9.3"
    },
    "keywords": [
        "Nimblebit",
        "Tiny Tower",
        "TinyTower"
    ],
    "exports": {
        ".": {
            "types": "./dist/tinyburg.d.ts",
            "import": "./dist/tinyburg.js"
        },
        "./data/*": {
            "types": "./dist/data/*.d.ts",
            "import": "./dist/data/*.js"
        },
        "./endpoints/*": {
            "types": "./dist/endpoints/*.d.ts",
            "import": "./dist/endpoints/*.js"
        },
        "./validation/*": {
            "types": "./dist/validation/*.d.ts",
            "import": "./dist/validation/*.js"
        },
        "./*.js": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js"
        }
    }
}
