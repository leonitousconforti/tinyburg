{
    "name": "@tinyburg/treasury",
    "version": "0.0.0",
    "description": "loans you coins and bux using costumes or floors as collateral for your investments",
    "keywords": [
        "nimblebit",
        "tinytower",
        "tinytowers"
    ],
    "homepage": "https://github.com/leonitousconforti/tinyburg",
    "bugs": {
        "url": "https://github.com/leonitousconforti/tinyburg/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "packages/treasury"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "scripts": {
        "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v2",
        "coverage": "vitest --coverage",
        "dtslint": "dtslint dtslint",
        "test": "vitest"
    },
    "dependencies": {
        "@effect/platform": "latest",
        "@effect/platform-node": "latest",
        "effect": "latest"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "provenance": true
    },
    "tags": [
        "nimblebit",
        "tinytower",
        "tinytowers"
    ]
}
