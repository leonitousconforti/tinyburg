{
    "name": "@tinyburg/tinytower-sdk",
    "version": "0.0.1",
    "description": "api for the mobile game TinyTower by Nimblebit",
    "keywords": [
        "nimblebit",
        "tinytower",
        "tinytowers"
    ],
    "homepage": "https://github.com/leonitousconforti/tinyburg",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "packages/tinytower-sdk"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "sideEffects": [],
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null
    },
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.build.json && babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v4",
        "generate": "node --experimental-strip-types ./scripts/GenerateData.ts && prettier --write 'src/internal/tinytower{BitbookPosts,Bitizens,Costumes,Elevators,Floors,Missions,Pets,Roofs}.ts'"
    },
    "dependencies": {
        "effect-schemas": "0.0.10",
        "pako": "2.1.0"
    },
    "devDependencies": {
        "@bufbuild/protobuf": "2.10.2",
        "@effect/cluster": "0.56.0",
        "@effect/experimental": "0.58.0",
        "@effect/platform": "0.94.0",
        "@effect/platform-node": "0.104.0",
        "@effect/rpc": "0.73.0",
        "@effect/sql": "0.49.0",
        "@effect/workflow": "0.16.0",
        "@efffrida/frida-tools": "0.0.23",
        "@efffrida/gplayapi": "0.0.4",
        "@tinyburg/insight": "workspace:*",
        "@types/pako": "2.0.4",
        "effect": "3.19.13"
    },
    "peerDependencies": {
        "@effect/platform": "0.93.8",
        "effect": "3.19.12"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            ".": "./dist/index.js",
            "./*": "./dist/*.js",
            "./internal/*": null
        }
    },
    "tags": [
        "nimblebit",
        "tinytower",
        "tinytowers"
    ]
}
