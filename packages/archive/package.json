{
    "name": "@tinyburg/archive",
    "version": "0.0.1",
    "description": "fetches any version of any of Nimblebit's Android apks directly from the playstore",
    "keywords": [
        "tinyburg",
        "nimblebit",
        "TinyTower",
        "Tiny Tower"
    ],
    "homepage": "https://github.com/leonitousconforti/tinyburg",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "packages/archive"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js"
        }
    },
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "_phase:build": "heft run --only build -- --clean",
        "_phase:test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft run --only test -- --clean",
        "build": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft test --clean",
        "postinstall": "node ./dist/download-latest-apks.cjs || true"
    },
    "dependencies": {
        "apk-mitm": "~1.2.1",
        "execa": "~8.0.1",
        "puppeteer": "~21.5.2"
    },
    "devDependencies": {
        "@effect/eslint-plugin": "0.1.2",
        "@effect/language-service": "0.0.21",
        "@effect/platform": "0.30.1",
        "@effect/platform-node": "0.31.1",
        "@effect/schema": "0.48.0",
        "@rushstack/eslint-config": "3.4.1",
        "@rushstack/heft": "0.63.2",
        "@rushstack/heft-jest-plugin": "0.10.4",
        "@rushstack/heft-lint-plugin": "0.2.12",
        "@rushstack/heft-typescript-plugin": "0.2.12",
        "@tinyburg/tsconfig-rig": "workspace:*",
        "@types/heft-jest": "1.0.6",
        "@types/node": "20.9.0",
        "effect": "2.0.0-next.55",
        "eslint": "8.53.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-unicorn": "49.0.0",
        "fast-check": "3.13.2",
        "prettier": "3.1.0",
        "typescript": "5.2.2"
    },
    "peerDependencies": {
        "@effect/platform": "~0.30.1",
        "@effect/platform-node": "~0.31.1",
        "effect": "~2.0.0-next.55"
    },
    "engines": {
        "node": ">=16"
    }
}
