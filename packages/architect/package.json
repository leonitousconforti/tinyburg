{
    "name": "@tinyburg/architect",
    "version": "0.0.1",
    "description": "Creates a TinyTower instance in an android emulator on a local or remote docker host",
    "keywords": [
        "tinyburg",
        "nimblebit",
        "TinyTower",
        "Tiny Tower"
    ],
    "homepage": "https://github.com/leonitousconforti/tinyburg",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "packages/architect"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./dist/src/index.js"
        },
        "./protobuf/*": {
            "types": "./dist/protobuf/*.d.ts",
            "import": "./dist/protobuf/*.js"
        },
        "./*": {
            "types": "./dist/src/*.d.ts",
            "import": "./dist/src/*.js"
        }
    },
    "main": "dist/src/index.js",
    "bin": {
        "architect": "./dist/src/cli.js"
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "_phase:build": "heft run --only build -- --clean",
        "_phase:test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft run --only test -- --clean",
        "build": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft test --clean",
        "protobuf": "buf generate"
    },
    "dependencies": {
        "@bufbuild/protobuf": "~1.9.0",
        "tar-fs": "~3.0.6",
        "the-moby-effect": "~1.45.0-alpha.5"
    },
    "devDependencies": {
        "@bufbuild/buf": "1.31.0",
        "@bufbuild/protoc-gen-es": "1.9.0",
        "@connectrpc/protoc-gen-connect-es": "1.4.0",
        "@effect/cli": "0.36.19",
        "@effect/eslint-plugin": "0.1.2",
        "@effect/platform": "0.53.0",
        "@effect/platform-node": "0.49.0",
        "@effect/printer": "0.33.11",
        "@effect/printer-ansi": "0.33.11",
        "@effect/schema": "0.66.14",
        "@effect/typeclass": "0.24.11",
        "@rushstack/eslint-config": "3.6.9",
        "@rushstack/heft": "0.66.4",
        "@rushstack/heft-jest-plugin": "0.11.25",
        "@rushstack/heft-lint-plugin": "0.3.25",
        "@rushstack/heft-typescript-plugin": "0.5.2",
        "@tinyburg/tsconfig-rig": "workspace:*",
        "@types/heft-jest": "1.0.6",
        "@types/node": "20.12.10",
        "@types/tar-fs": "2.0.4",
        "effect": "3.1.2",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-unicorn": "52.0.0",
        "execa": "~8.0.1",
        "fast-check": "3.18.0",
        "prettier": "3.2.5",
        "typescript": "5.4.5",
        "ws": "8.17.0"
    },
    "peerDependencies": {
        "@effect/platform": "0.53.0",
        "@effect/platform-node": "0.49.0",
        "@effect/schema": "0.66.14",
        "effect": "3.1.2"
    },
    "engines": {
        "node": ">=18"
    }
}
