{
    "name": "@tinyburg/architect",
    "version": "0.0.1-alpha",
    "description": "Architects a TinyTower instance in an android emulator or on a remote device",
    "keywords": [
        "tinyburg",
        "nimblebit",
        "TinyTower",
        "Tiny Tower"
    ],
    "homepage": "https://github.com/leonitousconforti/tinyburg",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "packages/architect"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./dist/src/index.js"
        },
        "./local": {
            "types": "./dist/src/local.d.ts",
            "import": "./dist/src/local.js"
        },
        "./ssh": {
            "types": "./dist/src/ssh.d.ts",
            "import": "./dist/src/ssh.js"
        },
        "./docker": {
            "types": "./dist/src/docker.d.ts",
            "import": "./dist/src/docker.js"
        }
    },
    "main": "dist/src/index.js",
    "bin": {
        "tinyburg-architect": "./dist/bin/cli.js"
    },
    "files": [
        "dist/",
        "device.xml"
    ],
    "scripts": {
        "build": "heft build --clean",
        "lint": "eslint ."
    },
    "dependencies": {
        "commander": "~9.4.1",
        "debug": "~4.3.4",
        "dockerode": "~3.3.4",
        "frida": "~16.0.7",
        "ssh2": "~1.11.0"
    },
    "devDependencies": {
        "@rushstack/eslint-config": "~3.1.1",
        "@rushstack/heft": "~0.48.8",
        "@tinyburg/tsconfig-rig": "workspace:*",
        "@types/debug": "~4.1.7",
        "@types/node": "~18.11.11",
        "@types/ssh2": "~1.11.6",
        "eslint": "~8.29.0",
        "eslint-config-prettier": "~8.5.0",
        "eslint-plugin-prettier": "~4.2.1",
        "eslint-plugin-unicorn": "~45.0.2",
        "prettier": "~2.8.1",
        "typescript": "~4.9.3"
    },
    "engines": {
        "node": ">=16.18.0"
    }
}
