{
    "name": "@tinyburg/apks",
    "version": "0.0.1-alpha.0",
    "description": "TinyTower apks downloaded from different sources, ready to be consumed by other tinyburg tools",
    "keywords": [
        "tinyburg",
        "nimblebit",
        "TinyTower",
        "Tiny Tower"
    ],
    "homepage": "https://github.com/leonitousconforti/tinyburg",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "packages/apks"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "main": "dist/index.js",
    "files": [
        "dist",
        "downloads",
        "!/downloads/**/*.apk"
    ],
    "scripts": {
        "_phase:build": "heft run --only build -- --clean",
        "_phase:test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft run --only test -- --clean",
        "build": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft test --clean",
        "postinstall": "node ./dist/postinstall.cjs || true"
    },
    "dependencies": {
        "debug": "~4.3.4",
        "got": "~13.0.0"
    },
    "devDependencies": {
        "@rushstack/eslint-config": "3.3.2",
        "@rushstack/heft": "0.59.0",
        "@rushstack/heft-jest-plugin": "0.9.0",
        "@rushstack/heft-lint-plugin": "~0.2.0",
        "@rushstack/heft-typescript-plugin": "~0.2.0",
        "@tinyburg/tsconfig-rig": "workspace:*",
        "@types/debug": "~4.1.8",
        "@types/heft-jest": "1.0.4",
        "@types/node": "20.6.2",
        "@types/prettier": "~2.7.2",
        "eslint": "8.45.0",
        "eslint-config-prettier": "~8.8.0",
        "eslint-plugin-prettier": "~4.2.1",
        "eslint-plugin-unicorn": "~47.0.0",
        "prettier": "~2.8.8",
        "puppeteer": "~20.8.2",
        "typescript": "5.1.6"
    },
    "engines": {
        "node": ">=16"
    }
}
