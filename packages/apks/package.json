{
    "name": "@tinyburg/apks",
    "version": "0.0.1",
    "description": "fetches Nimblebit apks from different sources, ready to be consumed by other tinyburg tools",
    "keywords": [
        "tinyburg",
        "nimblebit",
        "TinyTower",
        "Tiny Tower"
    ],
    "homepage": "https://github.com/leonitousconforti/tinyburg",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "packages/apks"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js"
        }
    },
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "_phase:build": "heft run --only build -- --clean",
        "_phase:test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft run --only test -- --clean",
        "build": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft test --clean",
        "postinstall": "node ./dist/download-latest-apks.cjs || true"
    },
    "dependencies": {
        "apk-mitm": "~1.2.1",
        "execa": "~8.0.1",
        "puppeteer": "~21.5.1"
    },
    "devDependencies": {
        "@effect/eslint-plugin": "0.1.2",
        "@effect/language-service": "0.0.21",
        "@effect/platform": "0.28.0",
        "@effect/platform-node": "0.29.0",
        "@effect/schema": "0.47.3",
        "@rushstack/eslint-config": "3.4.1",
        "@rushstack/heft": "0.63.1",
        "@rushstack/heft-jest-plugin": "0.10.3",
        "@rushstack/heft-lint-plugin": "0.2.11",
        "@rushstack/heft-typescript-plugin": "0.2.11",
        "@tinyburg/tsconfig-rig": "workspace:*",
        "@types/heft-jest": "1.0.6",
        "@types/node": "20.9.0",
        "effect": "2.0.0-next.54",
        "eslint": "8.53.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-unicorn": "49.0.0",
        "fast-check": "3.13.2",
        "prettier": "3.0.3",
        "typescript": "5.2.2"
    },
    "peerDependencies": {
        "@effect/platform": "~0.28.0",
        "@effect/platform-node": "~0.29.0",
        "effect": "~2.0.0-next.54"
    },
    "engines": {
        "node": ">=16"
    }
}
