{
    "compilerOptions": {
        "strict": true,
        "exactOptionalPropertyTypes": true,
        "moduleDetection": "force",
        "composite": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "esModuleInterop": false,
        "declaration": true,
        "skipLibCheck": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "NodeNext",
        "lib": ["ES2022", "DOM", "DOM.Iterable"],
        "types": [],
        "isolatedModules": true,
        "sourceMap": true,
        "declarationMap": true,
        "noImplicitReturns": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "noEmitOnError": false,
        "noErrorTruncation": false,
        "allowJs": false,
        "checkJs": false,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUncheckedIndexedAccess": false,
        "strictNullChecks": true,
        "baseUrl": ".",
        "target": "ES2022",
        "module": "NodeNext",
        "incremental": true,
        "removeComments": false,
        "plugins": [{ "name": "@effect/language-service" }],
        "paths": {
            "@tinyburg/bitprints": ["./packages/bitprints/src/index.ts"],
            "@tinyburg/bitprints/*": ["./packages/bitprints/src/*.ts"],
            "@tinyburg/bitprints/test/*": ["./packages/bitprints/test/*.ts"],
            "@tinyburg/doorman": ["./packages/doorman/src/index.ts"],
            "@tinyburg/doorman/*": ["./packages/doorman/src/*.ts"],
            "@tinyburg/doorman/test/*": ["./packages/doorman/test/*.ts"],
            "@tinyburg/explorer": ["./packages/explorer/src/index.ts"],
            "@tinyburg/explorer/*": ["./packages/explorer/src/*.ts"],
            "@tinyburg/explorer/test/*": ["./packages/explorer/test/*.ts"],
            "@tinyburg/fount": ["./packages/fount/src/index.ts"],
            "@tinyburg/fount/*": ["./packages/fount/src/*.ts"],
            "@tinyburg/fount/test/*": ["./packages/fount/test/*.ts"],
            "@tinyburg/insight": ["./packages/insight/src/index.ts"],
            "@tinyburg/insight/*": ["./packages/insight/src/*.ts"],
            "@tinyburg/insight/test/*": ["./packages/insight/test/*.ts"],
            "@tinyburg/nucleus": ["./packages/nucleus/src/index.ts"],
            "@tinyburg/nucleus/*": ["./packages/nucleus/src/*.ts"],
            "@tinyburg/nucleus/test/*": ["./packages/nucleus/test/*.ts"],
            "@tinyburg/treasury": ["./packages/treasury/src/index.ts"],
            "@tinyburg/treasury/*": ["./packages/treasury/src/*.ts"],
            "@tinyburg/treasury/test/*": ["./packages/treasury/test/*.ts"]
        }
    }
}
