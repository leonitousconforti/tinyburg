        "@protobuf-ts/runtime-rpc": "~2.9.0",
        "buffer": "~6.0.3",
        "react": "~18.2.0",
        "react-dom": "~18.2.0",
        "web-vitals": "~3.3.2"
    },
    "devDependencies": {
        "@babel/core": "~7.22.1",
        "@babel/plugin-syntax-flow": "~7.21.4",
        "@babel/plugin-transform-react-jsx": "~7.22.3",
        "@babel/preset-env": "~7.22.4",
        "@babel/preset-react": "~7.22.3",
        "@emotion/react": "~11.11.0",
        "@emotion/styled": "~11.11.0",
        "@protobuf-ts/plugin": "~2.9.0",
        "@protobuf-ts/protoc": "~2.9.0",
        "@rushstack/eslint-config": "3.3.0",
        "@rushstack/heft": "0.50.7",
        "@rushstack/heft-jest-plugin": "0.5.13",
        "@rushstack/heft-sass-plugin": "0.9.3",
        "@rushstack/heft-webpack5-plugin": "0.6.12",
        "@tinyburg/tsconfig-rig": "workspace:*",
        "@types/heft-jest": "1.0.3",
        "@types/node": "~20.2.5",
        "@types/react": "~18.2.7",
        "@types/react-dom": "~18.2.4",
        "@types/webpack-env": "~1.18.1",
        "autoprefixer": "~10.4.14",
        "babel-loader": "~9.1.2",
        "css-loader": "~6.8.1",
