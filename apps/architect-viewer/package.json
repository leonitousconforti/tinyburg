{
    "name": "@tinyburg/architect-viewer",
    "version": "0.0.1",
    "private": true,
    "description": "Stream and interact with a remote tinytower instance through the browser",
    "keywords": [
        "tinyburg",
        "nimblebit",
        "TinyTower",
        "Tiny Tower"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "apps/architect-viewer"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "files": [
        "build/"
    ],
    "scripts": {
        "build": "heft build --clean",
        "serve": "serve -s build",
        "start": "heft start --clean",
        "test": "heft test --clean",
        "types:clean": "rimraf src/generated/*",
        "types:gen": "protoc -I ./proto --ts_out ./src/generated --ts_opt optimize_code_size ./proto/emulator_controller.proto ./proto/rtc_service.proto"
    },
