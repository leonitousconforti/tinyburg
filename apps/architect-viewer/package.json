{
    "name": "@tinyburg/architect-viewer",
    "version": "0.0.1",
    "private": true,
    "description": "Stream and interact with a remote tinytower instance through the browser",
    "keywords": [
        "tinyburg",
        "nimblebit",
        "TinyTower",
        "Tiny Tower"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/tinyburg.git",
        "directory": "apps/architect-viewer"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "files": [
        "build/"
    ],
    "scripts": {
        "build": "react-scripts build",
        "clean-types": "rimraf src/generated/*",
        "eject": "react-scripts eject",
        "gen-types": "protoc -I ./proto --ts_out ./src/generated --ts_opt optimize_code_size ./proto/emulator_controller.proto ./proto/rtc_service.proto",
        "serve": "serve -s build",
        "start": "react-scripts start"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@emotion/react": "~11.11.0",
        "@emotion/styled": "~11.11.0",
        "@protobuf-ts/grpcweb-transport": "~2.9.0",
        "@protobuf-ts/runtime": "~2.9.0",
        "@protobuf-ts/runtime-rpc": "~2.9.0",
        "buffer": "~6.0.3",
        "byte-base64": "~1.1.0",
        "react": "~18.2.0",
        "react-dom": "~18.2.0",
        "react-is": "~18.2.0",
        "react-resize-detector": "~9.1.0",
        "web-vitals": "~3.3.1"
    },
    "devDependencies": {
        "@babel/core": "~7.21.8",
        "@babel/plugin-syntax-flow": "~7.21.4",
        "@babel/plugin-transform-react-jsx": "~7.21.5",
        "@protobuf-ts/plugin": "~2.9.0",
        "@protobuf-ts/protoc": "~2.9.0",
        "@tinyburg/tsconfig-rig": "workspace:*",
        "@types/node": "~20.1.3",
        "@types/react": "~18.2.6",
        "@types/react-dom": "~18.2.4",
        "rimraf": "~5.0.0",
        "serve": "~14.2.0",
        "typescript": "~5.0.4"
    }
}
