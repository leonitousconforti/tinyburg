{
    "private": true,
    "type": "module",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.build.json && pnpm --recursive --parallel --filter \"./packages/**/*\" run build",
        "changeset-publish": "pnpm build && TEST_DIST= pnpm vitest && changeset publish",
        "changeset-version": "changeset version",
        "check": "tsc -b tsconfig.json",
        "check-recursive": "pnpm --recursive --filter \"./packages/**/*\" exec tsc -b tsconfig.json",
        "circular": "node scripts/circular.mjs",
        "clean": "node scripts/clean.mjs",
        "codegen": "pnpm --recursive --parallel --filter \"./packages/**/*\" run codegen",
        "coverage": "vitest --coverage",
        "docgen": "pnpm --recursive --filter \"./packages/**/*\" exec docgen && node scripts/docs.mjs",
        "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
        "lint-fix": "pnpm lint --fix",
        "prepare": "effect-language-service patch",
        "test": "vitest"
    },
    "devDependencies": {
        "@babel/cli": "7.28.6",
        "@babel/core": "7.28.6",
        "@babel/plugin-transform-export-namespace-from": "7.27.1",
        "@babel/plugin-transform-modules-commonjs": "7.28.6",
        "@changesets/changelog-github": "0.5.2",
        "@changesets/cli": "2.29.8",
        "@effect/build-utils": "0.8.9",
        "@effect/docgen": "https://pkg.pr.new/Effect-TS/docgen/@effect/docgen@49e99a2",
        "@effect/language-service": "0.64.1",
        "@eslint/js": "9.39.2",
        "@typescript-eslint/eslint-plugin": "8.53.0",
        "@typescript-eslint/parser": "8.53.0",
        "@vitest/coverage-v8": "4.0.17",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "changesets-gitlab": "0.13.4",
        "eslint": "9.39.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-codegen": "0.34.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "glob": "13.0.0",
        "madge": "8.0.0",
        "prettier": "3.7.4",
        "prettier-plugin-jsdoc": "1.8.0",
        "prettier-plugin-packagejson": "2.5.20",
        "tsx": "4.21.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.53.0",
        "vite": "7.3.1",
        "vite-tsconfig-paths": "6.0.4",
        "vitest": "4.0.17"
    },
    "packageManager": "pnpm@9.15.9"
}
